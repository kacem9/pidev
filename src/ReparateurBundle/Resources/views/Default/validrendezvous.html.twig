{% extends "ReparateurBundle::layout.html.twig" %}
{% set title="Rendez vous" %}
{% block css %}




</style>
{% endblock %}
{% block body %}






    <body>

    <header>
        <h1>Appointment</h1>
    </header>












    <div class="w3ls-contact">

        <!-- form starts here -->
        <form action="#" method="post">
            {{ form_start(form) }}
            <div class="agile-field-txt">

                <div class="mr_agilemain">

                    <div class="left-wthree"><label class="bot_w3">
                            Date time</label>

                        {{ form_widget(form. dateheure) }}
                    </div>
                    <div class="left-wthree"> <label class="bot_w3">
                            Promo</label>

                        {{ form_widget(form. promo) }}
                    </div>
                </div>
            </div>
            <div class="agile-field-txt">
                <label>
                    State:</label>

                {{ form_widget(form.etat) }}
            </div>


            <div class="agile-field-txt">
                <label>
                    Price</label>
                {{ form_widget(form.prix) }}

            </div>
            <div class="agile-field-txt">
                <label>
                    Message</label>
                {{ form_widget(form.message) }}

            </div>


            <div class="w3ls-contact  w3l-sub">

                {{ form_widget(form.Ajouter) }}

            </div>
            {{ form_end(form) }}
        </form>
    </div>


    </body>
    <!-- //Body -->

    </html>
{% endblock %}